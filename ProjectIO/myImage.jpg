			.dotstyle {
				position:absolute;
				top:0;
				right:0;
				bottom:0;
				left:0;
				margin:auto;
				height:10px;
				width:23%;
				text-align:center;
			}
			
			.dotstyle ul {
				position: relative;
				display: inline-block;
				margin: 0;
				padding: 0;
				list-style: none;
				cursor: default;
				-webkit-touch-callout: none;
				-webkit-user-select: none;
				-khtml-user-select: none;
				-moz-user-select: none;
				-ms-user-select: none;
				user-select: none;
			}

			.dotstyle li {
				position: relative;
				display: block;
				float: left;
				margin: 0 3px;
				width: 10px;
				height: 10px;
				cursor: pointer;
			}

			.dotstyle li .dotitem {
				top: 0;
				left: 0;
				width: 100%;
				height: 100%;
				outline: none;
				border-radius: 50%;
				background-color: #fff;
				text-indent: -999em; /* make the text accessible to screen readers */
				cursor: pointer;
				position: absolute;
			}

			.dotstyle li .dotitem:focus {
				outline: none;
			}

			/* Individual styles and effects */
			/* Scale up */

			.dotstyle-scaleup li .dotitem {
				-webkit-transition: -webkit-transform 0.3s ease, background-color 0.3s ease;
				transition: transform 0.3s ease, background-color 0.3s ease;
			}

			.dotstyle-scaleup li .dotitem:hover,
			.dotstyle-scaleup li .dotitem:focus {
				background-color: white;
			}

			.dotstyle-scaleup li.current .dotitem {
				-webkit-transform: scale(1.5);
				/*transform: scale(1.5);*/
			}	
		



